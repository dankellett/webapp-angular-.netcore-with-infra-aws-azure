{
  "ConnectionStrings": {
    "DefaultConnection": "#{db_connectionstring}#"
  },
  "Logging": {
      "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
      }
    },
  "IdentityServer": {
    "Clients": {
      "app_template": {
        "Profile": "IdentityServerSPA"
      }
    },
    "Key": {
      "Type": "File",
      "FilePath": ".\\auth_key_dev.pfx",
      "Password": "#{auth_cert_password}"
    }
  },
"AllowedHosts": "*"
}
