{
  "ConnectionStrings": {
    "DefaultConnection": "#{db_connectionstring}#"
  },
  "Logging": {
      "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
      }
    },
  "IdentityServer": {
    "Clients": {
      "app_template": {
        "Profile": "IdentityServerSPA"
      }
    },
    "Key": {
      "Type": "Store",
      "StoreName": "My",
      "StoreLocation": "#{auth_cert_store_location}",
      "Name": "CN=example.com"
    }
  },
"AllowedHosts": "*"
}
